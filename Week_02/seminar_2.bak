#lang racket/base
;Задача 1. Да се напише функция mygcd a b, която връща НОД(a, b).

(define (mygcd a b)
  (cond
        [(= a 0) b]
        [(= b 0) a])
        [(> a b)  (mygcd (- a b) b)]
        [(<= a b) (mygcd a (- b a))]
        
  )

(mygcd 5 10)
      

